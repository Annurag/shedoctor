<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/app_grid_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.indglobal.shedoctor.Fonts.GouthamCheck
        android:id="@+id/tvLangtype"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="@drawable/checkbox_slctr"
        android:textAlignment="center"
        android:button="@null"
        android:textColor="@color/color_selector_new"
        android:layout_margin="2dp"
        android:textSize="12sp"/>

    <com.indglobal.shedoctor.Fonts.GouthamBookText
        android:id="@+id/tvBooked"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:gravity="center"
        android:background="@drawable/booked"
        android:textColor="@color/fullGray"
        android:layout_margin="2dp"
        android:textSize="12sp"
        android:visibility="gone"/>
</RelativeLayout>
